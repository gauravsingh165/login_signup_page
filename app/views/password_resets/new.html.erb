<h1>Forgot Password</h1>
<%= form_with url: password_reset_path do |form|%>
  <div class ="mb-3" style="background-color:#FF9933 ;">
    <%= form.label :email %><br>
    <%= form.text_field :email, class: "form-control", placeholder: "gaurav123@gmail.com" %>
  </div>
 <div  class ="mb-3">
   <%= form.submit "Reset Password",class: "btn btn-primary", "data-turbo"=>"false"%>
 </div>
 <%end%>
